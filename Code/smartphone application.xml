<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <Button
        android:id="@+id/button4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Sign up for vaccination"
        tools:layout_editor_absoluteX="87dp"
        tools:layout_editor_absoluteY="285dp" />

    <Button
        android:id="@+id/button3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Sign up for test"
        tools:layout_editor_absoluteX="120dp"
        tools:layout_editor_absoluteY="237dp" />

    <EditText
        android:id="@+id/editTextTextPersonName"
        android:layout_width="356dp"
        android:layout_height="46dp"
        android:ems="10"
        android:inputType="textPersonName"
        android:singleLine="false"
        android:text="Aerial Drone Corona Test/Vaccine Transport service"
        android:textSize="14sp"
        tools:layout_editor_absoluteX="27dp"
        tools:layout_editor_absoluteY="47dp" />

    <EditText
        android:id="@+id/editTextTextEmailAddress"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:ems="10"
        android:hint="Enter email address"
        android:inputType="textEmailAddress"
        android:text="myemail@gmail.com"
        tools:layout_editor_absoluteX="101dp"
        tools:layout_editor_absoluteY="439dp" />

    <EditText
        android:id="@+id/editTextTextPassword"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:ems="10"
        android:hint="Enter password"
        android:inputType="textPassword"
        android:text="password"
        tools:layout_editor_absoluteX="101dp"
        tools:layout_editor_absoluteY="500dp" />

    <EditText
        android:id="@+id/editTextTextPostalAddress"
        android:layout_width="234dp"
        android:layout_height="40dp"
        android:ems="10"
        android:hint="Enter delivery co-ordinates"
        android:inputType="textPostalAddress"
        android:text="52.4465° N, 0.6225° E"
        tools:layout_editor_absoluteX="27dp"
        tools:layout_editor_absoluteY="629dp" />

    <Button
        android:id="@+id/button2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="36dp"
        android:layout_marginRight="36dp"
        android:layout_marginBottom="172dp"
        android:text="launch*"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent" />

</androidx.constraintlayout.widget.ConstraintLayout>